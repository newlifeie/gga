rid">\n          <div class="selectable-product-option-container">\n            <div class="available-product-option-slots">\n              <div\n                class="slot-container"\n                ng-repeat="option in $ctrl.getOptionNumber(option.option_max_selection) track by $index"\n              >\n                <div class="slot"></div>\n              </div>\n            </div>\n            <div class="selected-product-options">\n              <product-option-button\n                block="product-option-button"\n                ng-repeat="o_value in (option.selectedValue | filter: $select.search) track by o_value.trackId"\n                is-multiple-select="true"\n                option-value="o_value"\n                is-selected="$ctrl.includes( option.selectedValue, o_value )"\n                is-disabled="false"\n                on-select="$ctrl.removeOption( option, $index )"\n                on-image-click="$ctrl.openOptionImageGallery( option.option_values, o_value )"\n              ></product-option-button>\n            </div>\n          </div>\n        </div>\n        <div class="updated-product-option-grid">\n          <product-option-button\n            block="product-option-button"\n            ng-repeat="o_value in (option.option_values | filter: $select.search)"\n            is-multiple-select="true"\n            option-value="o_value"\n            is-selected="false"\n            is-disabled="$ctrl.isOptionValueDisabled( option )"\n            on-select="$ctrl.updateOption( option, o_value )"\n            on-image-click="$ctrl.openOptionImageGallery( option.option_values, o_value )"\n            ng-init="$ctrl.checkFormState()"\n          ></product-option-button>\n        </div>\n      </fieldset>\n      <textarea\n        ng-if="option.option_type === \'freetext\' && !option.option_multiple"\n        name="{{ option.option_name }}"\n        aria-label="Personalized Message"\n        class="option-text form-control"\n        ng-model="op